CT_CONFIG_VERSION="3"
CT_EXPERIMENTAL=y
# CT_PREFIX_DIR_RO is not set
CT_CREATE_TARBALL=y
CT_EXTRA_CFLAGS_FOR_HOST="-O2"
# CT_LOG_TO_FILE is not set
CT_ARCH_ARM=y
CT_ARCH_64=y
CT_TARGET_CFLAGS="-pipe -O2 -mcpu=cortex-a53 -g1 -fno-omit-frame-pointer"
CT_TOOLCHAIN_PKGVERSION="NDMS Toolkit 1.0.0"
CT_TOOLCHAIN_BUGURL="https://forum.keenetic.com"
CT_TARGET_VENDOR="ndms"
CT_TARGET_VENDOR_SAMPLE_REV="4.00"
CT_KERNEL_LINUX=y
CT_LINUX_SRC_DEVEL=y
CT_LINUX_DEVEL_URL="https://github.com/ndmsystems/kernel-49.git"
CT_LINUX_DEVEL_BRANCH="4.9.337-37"
CT_LINUX_V_4_9=y
CT_BINUTILS_PLUGINS=y
CT_LIBC_GLIBC=y
CT_GCC_V_12=y
CT_GLIBC_DISABLE_VERSIONING=y
CT_GLIBC_KERNEL_VERSION_CHOSEN=y
CT_GLIBC_MIN_KERNEL_VERSION="4.9.255"
CT_CC_GCC_CONFIG_TLS=y
CT_CC_GCC_LIBSSP=y
CT_CC_GCC_LIBSANITIZER=y
CT_CC_LANG_CXX=y
CT_DEBUG_GDB=y
CT_GDB_V_8_3_1=y
# CT_GDB_CROSS_PYTHON is not set
CT_GDB_NATIVE=y
CT_GDB_NATIVE_EXTRA_CONFIG_ARRAY="--disable-gdbmi --with-expat=no --enable-tui=no --with-curses=no"
CT_GDB_NATIVE_EXTRA_CFLAGS="-Os -ffunction-sections -fdata-sections"
CT_GDB_NATIVE_EXTRA_LDFLAGS="-Wl,--gc-sections"
CT_DEBUG_LTRACE=y
CT_DEBUG_STRACE=y
