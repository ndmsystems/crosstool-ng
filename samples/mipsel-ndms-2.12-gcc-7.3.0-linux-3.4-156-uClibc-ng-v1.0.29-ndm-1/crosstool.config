CT_EXPERIMENTAL=y
# CT_PREFIX_DIR_RO is not set
CT_CREATE_TARBALL=y
CT_PARALLEL_JOBS=16
CT_EXTRA_CFLAGS_FOR_HOST="-O2"
CT_ARCH_MIPS=y
CT_ARCH_TUNE="mips32r2"
CT_ARCH_SUFFIX="el"
CT_ARCH_LE=y
CT_ARCH_FLOAT_SW=y
CT_TARGET_CFLAGS="-pipe -O2 -mips32r2"
CT_TOOLCHAIN_PKGVERSION="NDMS Toolkit 1.0.0"
CT_TOOLCHAIN_BUGURL="https://forum.keenetic.net/"
CT_TARGET_VENDOR="ndms"
CT_TARGET_VENDOR_SAMPLE_REV="2.12"
CT_KERNEL_LINUX=y
CT_LINUX_SRC_DEVEL=y
CT_LINUX_DEVEL_URL="https://github.com/ndmsystems/linux-3.4.git"
CT_LINUX_DEVEL_REVISION="3.4-156"
CT_LINUX_V_3_4=y
CT_LIBC_UCLIBC=y
CT_UCLIBC_NG_SRC_DEVEL=y
CT_UCLIBC_NG_DEVEL_URL="https://github.com/ndmsystems/uClibc-ng.git"
CT_UCLIBC_NG_DEVEL_REVISION="v1.0.29-ndm-1"
CT_LIBC_UCLIBC_CONFIG_FILE="${CT_LIB_DIR}/samples/mipsel-ndms-2.12-gcc-7.3.0-linux-3.4-156-uClibc-ng-v1.0.29-ndm-1/uClibc-ng-v1.0.29-ndm-1.config"
CT_LIBC_UCLIBC_LOCALES=y
CT_LIBC_UCLIBC_IPV6=y
CT_CC_GCC_CONFIG_TLS=y
CT_CC_GCC_LIBSSP=y
CT_CC_GCC_LIBSANITIZER=y
CT_CC_LANG_CXX=y
CT_DEBUG_GDB=y
CT_GDB_V_7_11_1=y
CT_GDB_NATIVE=y
CT_GDB_NATIVE_EXTRA_CONFIG_ARRAY="--disable-gdbmi --with-expat=no --enable-tui=no --with-curses=no"
CT_DEBUG_LTRACE=y
CT_DEBUG_STRACE=y
