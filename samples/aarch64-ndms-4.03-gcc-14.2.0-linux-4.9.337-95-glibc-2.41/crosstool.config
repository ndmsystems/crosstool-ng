CT_CONFIG_VERSION="4"
CT_EXPERIMENTAL=y
# CT_PREFIX_DIR_RO is not set
CT_TARBALL_RESULT=y
CT_TARBALL_RESULT_FILENAME="toolchain_${CT_VERSION}_${CT_SYS_TARGET}_${CT_TARGET}"
CT_EXTRA_CFLAGS_FOR_BUILD="-O2"
CT_ARCH_ARM=y
CT_ARCH_64=y
CT_TARGET_CFLAGS="-pipe -O2 -mcpu=cortex-a53 -g1 -fno-omit-frame-pointer"
CT_TOOLCHAIN_BUGURL="https://ndmsystems.com/"
CT_TARGET_VENDOR="ndms"
CT_TARGET_VENDOR_SAMPLE_REV="4.03"
CT_KERNEL_LINUX=y
CT_LINUX_SRC_DEVEL=y
CT_LINUX_DEVEL_URL="https://github.com/ndmsystems/kernel-49.git"
CT_LINUX_DEVEL_BRANCH="4.9.337-95"
CT_LINUX_V_4_9=y
CT_BINUTILS_PLUGINS=y
CT_GCC_V_14=y
CT_GLIBC_DISABLE_VERSIONING=y
CT_CC_GCC_CONFIG_TLS=y
CT_CC_GCC_LIBSSP=y
CT_CC_GCC_LIBSANITIZER=y
CT_CC_LANG_CXX=y
CT_DEBUG_GDB=y
CT_GDB_V_8_3=y
# CT_GDB_CROSS_PYTHON is not set
CT_GDB_NATIVE=y
CT_GDB_NATIVE_EXTRA_CONFIG_ARRAY="--disable-gdbmi --with-expat=no --enable-tui=no --with-curses=no"
CT_GDB_NATIVE_EXTRA_CFLAGS="-Os -ffunction-sections -fdata-sections"
CT_GDB_NATIVE_EXTRA_LDFLAGS="-Wl,--gc-sections"
CT_DEBUG_LTRACE=y
CT_DEBUG_STRACE=y
